<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="vb" alignment="TOP_CENTER" fillWidth="false" prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns:fx="http://javafx.com/fxml" fx:controller="yamb.YambForm">
  <!-- TODO Add Nodes -->
  <children>
    <Button fx:id="btnBaci" mnemonicParsing="false" text="Baci preostale kockice" />
    <FlowPane alignment="CENTER" hgap="5.0" prefHeight="200.0" prefWidth="-1.0" prefWrapLength="500.0" vgap="10.0">
      <children>
        <ImageView fx:id="iv" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
        <Label text="Broj bacanja:" />
        <TextField fx:id="txfBrBacanja" alignment="CENTER" editable="false" focusTraversable="false" prefColumnCount="2" prefWidth="-1.0" />
        <Label fx:id="lblRezultat" prefWidth="200.0" text="" />
      </children>
    </FlowPane>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</VBox>
